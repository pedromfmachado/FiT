<div class="span14">
  <h1>Exercício</h1>

  <div class="row spaced-lines">
    <div class="span9">

      <div class="row">
        <div class="span3 right-align">
          <strong>Nome:</strong>
        </div>
        <div class="span6">
          <%= @exercicio.nome %>
        </div>
      </div>

      <div class="row">
        <div class="span3 right-align">
          <strong>Maquina:</strong>
        </div>
        <div class="span6">
          <%= @exercicio.maquina %>
        </div>
      </div>

      <div class="row">
        <div class="span3 right-align">
          <strong>Tipo:</strong>
        </div>
        <div class="span6">
          <%= @exercicio.tipo %>
        </div>
      </div>
    </div>
  </div>

  <div class="actions topSpacing2">
    <%= link_to 'Editar Exercício', edit_exercicio_path(@exercicio), :class => "btn primary" %>
    <%= link_to 'Voltar', exercicios_path, :class => "btn" %>
  </div>
</div>

